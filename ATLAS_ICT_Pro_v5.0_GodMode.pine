nyPMKZ = nyHour >= 13 and nyHour < 15

// Silver Bullet Windows
sb1 = nyHour == 3 and nyMinute >= 0 and nyMinute < 15
sb2 = nyHour == 10 and nyMinute >= 0 and nyMinute < 15
sb3 = nyHour == 14 and nyMinute >= 0 and nyMinute < 15
