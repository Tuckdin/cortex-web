             "MS+" + (htfBias == DIR_BULL ? " HTF+" : "") +
             (inDiscount ? " Disc+" : "") + (inOTE ? " OTE+" : "") +
             (bullishTS ? " TS+" : "") + (model2022Bull ? " 2022+" : "") :
             dolDirection == DIR_BEAR ?
             "MS-" + (htfBias == DIR_BEAR ? " HTF-" : "") +
             (inPremium ? " Prem-" : "") + (inOTE ? " OTE-" : "") +
